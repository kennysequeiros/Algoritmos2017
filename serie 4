// serie +4.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include<iostream>
#include"conio.h"
using namespace std;
void main()
{int s=0, N;
do{
	cout <<"ingresar el valor de N";
	cin >> N;
	
}while(N<=0);
for (int i=1;i<=N;i=i+4)
	s=s+i;
cout <<"la suma es: " <<s;
getch();}
